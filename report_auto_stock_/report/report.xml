<?xml version="1.0"?>
<openerp>
<data noupdate="1">

    <record id="aeroo_store_auto_report_id" model="ir.actions.report.xml">
        <field name="name">Auto report store supplier</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">statistic.store</field>
        <field name="report_name">store_auto_report</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="parser_loc">report_auto_stock_/report/parser.py</field>
        <field name="report_rml">report_auto_stock_/report/store.odt</field>
        <field name="parser_state">loc</field>
        <field name="tml_source">file</field>
        <field name="out_format" eval="ref('report_aeroo_ooo.report_mimetypes_pdf_odt')"></field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['statistic.store']" name="models"/>
        <field name="name">store_auto_report_action</field>
        <field eval="'ir.actions.report.xml,'+str(aeroo_store_auto_report_id)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

</data>
</openerp>

